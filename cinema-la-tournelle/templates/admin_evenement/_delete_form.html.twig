<form method="post" action="{{ path('app_admin_evenement_delete', {'id': evenement.id}) }}" onsubmit="return confirm('Etes-vous sûr de vouloir supprimer cet événement?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ evenement.id) }}">
    <button class="btn btn-inverse-danger me-2">Supprimer</button>
</form>
